# Server Configuration
NODE_ENV=development
PORT=5000

# Frontend URL (for CORS - optional, defaults to localhost)
# In production, set this to your frontend URL (e.g., https://your-app.com)
FRONTEND_URL=http://localhost:5173

# JWT Secrets
# Generate secure random strings for production:
# node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
JWT_SECRET=your-secret-key-here-generate-a-random-32-byte-hex-string
ADMIN_JWT_SECRET=your-admin-secret-key-here-generate-a-random-32-byte-hex-string

# Admin Credentials
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=change-this-password
